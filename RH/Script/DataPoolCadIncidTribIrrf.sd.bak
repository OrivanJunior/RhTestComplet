var
CadITDPIdIncTribIrrf,
CadITDPDescricao,
CadITDPDtInivalidade,
CadITDPDtFinValidade   : OleVariant;

procedure LeDataPoolCadITInss(nomeDriver);
begin
CadITDPIdIncTribIrrf  := DDT.DriverByName(nomeDriver).Value('IDINCTRIBINSS');
CadITDPDescricao      := DDT.DriverByName(nomeDriver).Value('DESCRICAO');

CadITDPDtIniValidade  := DDT.DriverByName(nomeDriver).Value('DTINIVALIDADE');
CadITDPDtFinValidade  := DDT.DriverByName(nomeDriver).Value('DTFINVALIDADE');
end;

