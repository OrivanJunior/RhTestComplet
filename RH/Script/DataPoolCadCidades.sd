var
CadCidadeDPCidade,
CadCidadeDPNome,
CadCidadeDPUf,
CadCidadeDPDdd,
CadCidadeDPOficial,
CadCidadeDPCep,
CadCidadeDPPais,
CadCidadeDPIbge,
CadCidadeDPCodDomicilio : OleVariant;

procedure LeDataPoolCadCidades(nomeDriver);
begin
CadCidadeDPCidade		    := DDT.DriverByName(nomeDriver).Value('CIDADE');          
CadCidadeDPNome			    := DDT.DriverByName(nomeDriver).Value('NOME');            
CadCidadeDPUf			      := DDT.DriverByName(nomeDriver).Value('UF');                
CadCidadeDPDdd			    := DDT.DriverByName(nomeDriver).Value('DDD');             
CadCidadeDPOficial		  := DDT.DriverByName(nomeDriver).Value('OFICIAL');       
CadCidadeDPCep			    := DDT.DriverByName(nomeDriver).Value('CEP');             
CadCidadeDPPais			    := DDT.DriverByName(nomeDriver).Value('PAIS');            
CadCidadeDPIbge			    := DDT.DriverByName(nomeDriver).Value('IBGE');            
CadCidadeDPCodDomicilio := DDT.DriverByName(nomeDriver).Value('CODDOMICILIO');
end;









