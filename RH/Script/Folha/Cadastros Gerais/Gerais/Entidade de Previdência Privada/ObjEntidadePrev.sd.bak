var
  FormCadEntidadePrev,
  FormCadEntidadePrevIdentidadeInfpc, 
  FormCadEntidadePrevCNPJ, 
  FormCadEntidadePrevRazaoSoc: OleVariant;

procedure MapTelaCadEntidadePrev();
begin
  FormCadEntidadePrev                := Aliases.Folha.FRhEntidadeINFPC;
     
  FormCadEntidadePrevIdentidadeInfpc := Aliases.Folha.FRhEntidadeINFPC.pnlPrincipal.EB_IDENTIDADEINFPC;
  FormCadEntidadePrevCNPJ            := Aliases.Folha.FRhEntidadeINFPC.pnlPrincipal.EB_CNPJ;
  FormCadEntidadePrevRazaoSoc        := Aliases.Folha.FRhEntidadeINFPC.pnlPrincipal.EB_RAZAOSOC;
end;
