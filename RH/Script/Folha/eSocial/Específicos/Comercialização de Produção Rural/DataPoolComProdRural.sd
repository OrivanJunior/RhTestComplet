procedure LeDataPoolCadComProdRural(nomeDriver);
begin
CadCompProdRuralDPEstab  						        := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPEstab');
CadCompProdRuralDPIdProdrural  	            := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPIdProdrural');
CadCompProdRuralDPMes  			                := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPMes');
CadCompProdRuralDPAno  			                := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPAno');
CadCompProdRuralDPIndcomerc  	              := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPIndcomerc');
CadCompProdRuralDPIdProcessoJud             := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPIdProcessoJud');
CadCompProdRuralDPVlrCpSusp  	              := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPVlrCpSusp');
CadCompProdRuralDPVlrRatSusp  	            := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPVlrRatSusp');
CadCompProdRuralDPVlrSenarSusp              := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPVlrSenarSusp');
CadCompProdRuralDPTotVlrComerc              := DDT.DriverByName(nomeDriver).Value('CadCompProdRuralDPTotVlrComerc');
end; 


