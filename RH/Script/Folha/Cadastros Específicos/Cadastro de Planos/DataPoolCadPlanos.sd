procedure LeDataPoolCadPlanos(nomeDriver);
begin
CadPlanosDPEstab  		  := DDT.DriverByName(nomeDriver).Value('ESTAB');
CadPlanosDPIdEmpregado  := DDT.DriverByName(nomeDriver).Value('IDEMPREGADO');
CadPlanosDPIdOpSaude	  := DDT.DriverByName(nomeDriver).Value('IDOPSAUDE');
end;

