procedure PreencheValidaSituacao(pathData);
var
DDTSituacao : OleVariant;
i : integer;
begin
i:= 0;
  CadFormPageTab.ActivePageIndex := 0;
  
  DDTSituacao := CarregaDDT(pathData, 'Situações');
  
  LeDataPoolCadEventos(DDTSituacao.Name);
  
  BtnIncSit.Click();
  
  Log.Checkpoint('Preenchendo Situações');
  
  VerificaClickPreencheGrid(GridSituacao, 0, 'Situacao', CadEventosDPCodigo);
  
  GridEmpregado.Click();
  
  BtnSaveEmpre.Click;

end;

