var    
CadProcDPIdCadProc,
CadProcDPDescricao,
CadProcDPCodManad : OleVariant;


procedure LeDataPoolTipoProc();
begin
CadProcDPIdCadProc    := DDT.DriverByName(nomeDriver).Value('IDCADPROC');
CadProcDPDescricao    := DDT.DriverByName(nomeDriver).Value('DESCRICAO');
CadProcDPCodManad     := DDT.DriverByName(nomeDriver).Value('CODMANAD');
end;
