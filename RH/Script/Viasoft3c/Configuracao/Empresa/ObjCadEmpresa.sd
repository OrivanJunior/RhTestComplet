  CadEmpresaEmpresa, 
  CadEmpresaRazaoSoc, 
  CadEmpresaReduzido, 
  CadEmpresaInativa: OleVariant;
  
procedure MapCadEmpresa();
begin
  CadEmpresaForm     := Aliases.Viasoft3c_XE.FFormPadraoEMPRESA;
