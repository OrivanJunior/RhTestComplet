  CadEmpresaReduzido := Aliases.Viasoft3c_XE.FFormPadraoEMPRESA.FFormPadraoCds_abPrinc.FFormPadraoCds_abPrinc_ts1.TScrollBox_.EB_REDUZIDO;
  CadEmpresaInativa  := Aliases.Viasoft3c_XE.FFormPadraoEMPRESA.FFormPadraoCds_abPrinc.FFormPadraoCds_abPrinc_ts1.TScrollBox_.EB_INATIVA;
end;

