var
CadCagedDPCodCagedResp,
CadCagedDPEstab,
CadCagedDPAutorizacao : OleVariant;


procedure LeDataPoolCadCaged(nomeDriver);
begin
CadCagedDPCodCagedResp	:= DDT.DriverByName(nomeDriver).Value('CODCAGEDRESP');
CadCagedDPEstab  		    := DDT.DriverByName(nomeDriver).Value('ESTAB');
CadCagedDPAutorizacao	  := DDT.DriverByName(nomeDriver).Value('AUTORIZACAO');
end;