var    
CadTipoProcDPIdCadProc,
CadTipoProcDPDescricao,
CadTipoProcDPCodManad : OleVariant;


procedure LeDataPoolTipoProc(nomeDriver);
begin
CadTipoProcDPIdCadProc    := DDT.DriverByName(nomeDriver).Value('IDCADPROC');
CadTipoProcDPDescricao    := DDT.DriverByName(nomeDriver).Value('DESCRICAO');
CadTipoProcDPCodManad     := DDT.DriverByName(nomeDriver).Value('CODMANAD');
end;
