var 
FormCadValFaixas,
PageCadTabNormalCab,
CadValFaixasEBIdValFaixas,
CadValFaixasEBDescricao,
                         
                         
PageCadTabNormalPg,
BtnIncGridValFaixas,
BtnProcGridValFaixas,
BtnConsulGridValFaixas,
BtnDesfGridValFaixas,
BtnExcGridValFaixas,
TabValfaixasGrid : OleVariant;

procedure MapCadFaixaCalc();
begin
FormCadValFaixas  		  	:= Aliases.Folha.FRhValFaixas;
PageCadTabNormalCab			  := Aliases.Folha.FRhValFaixas.Page.TabNormal.pnCab;
CadValFaixasEBIdValFaixas	:= Aliases.Folha.FRhValFaixas.Page.TabNormal.pnCab.EB_IDVALFAIXAS;
CadValFaixasEBDescricao  	:= Aliases.Folha.FRhValFaixas.Page.TabNormal.pnCab.EB_DESCRICAO;
  
  
PageCadTabNormalPg		    := Aliases.Folha.FRhValFaixas.Page.TabNormal.pg;
BtnIncGridValFaixas 	    := Aliases.Folha.FRhValFaixas.bInc;
BtnProcGridValFaixas 	    := Aliases.Folha.FRhValFaixas.bProc;
BtnConsulGridValFaixas    := Aliases.Folha.FRhValFaixas.bConsul;
BtnDesfGridValFaixas 	    := Aliases.Folha.FRhValFaixas.bDesf;
BtnExcGridValFaixas  	    := Aliases.Folha.FRhValFaixas.bExc;
TabValfaixasGrid  		    := Aliases.Folha.FRhValFaixas.Page.TabNormal.pg.tsRhRaisEve.sbRHExpDet.gValFaixas.TcxGridSite;
end;


procedure IncluirGrid();
var 
FormTelaConsultaGrid;
begin
FormTelaConsultaGrid :=  Sys.Process('Folha').VCLObject('FRhValFaixas').VCLObject('Page').VCLObject('TabNormal').VCLObject('pg');
FormTelaConsultaGrid.ExecuteAction(IncluirGrid);
end;
