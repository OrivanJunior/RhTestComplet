var
CadOrgaoEmissorDPIdOrgao,
CadOrgaoEmissorDPSigla,
CadOrgaoEmissorDPDescricao : OleVariant;


procedure LeDataPoolCadOrgaoEmissor(nomeDriver);
begin
CadOrgaoEmissorDPIdOrgao    := DDT.DriverByName(nomeDriver).Value('IDORGAO');
CadOrgaoEmissorDPSigla      := DDT.DriverByName(nomeDriver).Value('SIGLA');
CadOrgaoEmissorDPDescricao  := DDT.DriverByName(nomeDriver).Value('DESCRICAO');
end;

