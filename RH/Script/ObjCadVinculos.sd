Var
VinculosEBForm,  			
VinculosEBFormPage,  	
                      
VinculosEBIdVinc,  			
VinculosEBCodRais,
VinculosEBCodSefip,  	
VinculosEBDescricao,  	
VinculosEBCodDarf,  		
VinculosEBCodDarfPlr,  
VinculosEBCodCategoria,
VinculosEBEmpPagSalMat,
VinculosEBTipoContrato : OleVariant;
 
 

procedure MapCadVinculos();
begin
VinculosEBForm  			  :=	Aliases.Folha.FRhVinculo;
VinculosEBFormPage  		:=	Aliases.Folha.FRhVinculo.Page;

VinculosEBIdVinc  			:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_IDVINC;
VinculosEBCodRais  			:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_CODRAIS;
VinculosEBCodSefip  		:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_CODSEFIP;
VinculosEBDescricao  		:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_DESCRICAO;
VinculosEBCodDarf  			:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_CODDARF;
VinculosEBCodDarfPlr  	:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_CODDARFPLR;
VinculosEBCodCategoria 	:=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_CODCATEGORIA;
VinculosEBEmpPagSalMat  :=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_EMPPAGSALMAT;
VinculosEBTipoContrato  :=	Aliases.Folha.FRhVinculo.Page.TabNormal.ScrollBox1.EB_TIPOCONTRATO;
end;